(module main)
(export main replicate)
(import std (Unit))
(import std/io (IO println))
(import std/list (List length))
(import std/nat (Nat elim-Nat))
(import std/string (String))
(import std/vect (Vect vnil vcons))

(defn replicate {T TYPE} (x T) (n Nat) -> (Vect T n)
  (elim-Nat (Vect T)
    vnil
    (fn (n tl) (vcons x tl))
    n))

(defn main (args (List String)) -> Unit
  (println (replicate "a" (length args))))
